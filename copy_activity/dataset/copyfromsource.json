{
	"name": "copyfromsource",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_azure8",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"game": {
				"type": "string",
				"defaultValue": "Bowling"
			},
			"playertype": {
				"type": "string",
				"defaultValue": "android"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().game,'.',dataset().playertype,'.csv')",
					"type": "Expression"
				},
				"fileSystem": "input"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}